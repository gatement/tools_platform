{application, interface_tcp,
 [{description, "TCP interface for tools_platform"},
  {vsn, "0.1.0"},
  {registered, [
		interface_tcp_sup, 
		interface_tcp_sup_ac, 
		interface_tcp_sup_controller]},
  {applications, [kernel, stdlib]},
  {env, [
    {tcp_port_device, 10000},
    {heartbeat_checking_interval_device, 120000} % in milliseconds
  ]},
  {mod, {interface_tcp_app, []}}
 ]}.
